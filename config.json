      ]
    },
    {
      "before": [
        "y",
        "i",
        "i"
      ],
      "after": [
        "y",
        "i",
        "\""
      ]
    },
    {
      "before": [
        "c",
        "i",
        "i"
      ],
      "after": [
        "c",
        "i",
        "\""
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "i",
        "<CR>",
        "<ESC>"
      ]
    },
    {
      "before": [
        "space"
      ],
      "after": [
        "/"
      ]
    },
